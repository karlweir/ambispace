SecondsSinceLastEnemySpawn = SecondsSinceLastEnemySpawn + 1/room_speed

if SecondsSinceLastEnemySpawn &gt;= SecondsBetweenEnemySpawns {
    //Spawn enemy
    if choose(true,false) {
    SpawnX = random(room_width)
    SpawnY = choose(0, room_height)
    } else {
    SpawnX = choose(0, room_width)
    SpawnY = random(room_height)
    }
    
    instance_create(SpawnX, SpawnY, choose(oRock))
    SecondsSinceLastEnemySpawn = 0
}

/* Screen Shake */
//If we've been told to shake the screen by any amount
if ScreenShakeIntensity &gt; 0 {
    //Maker sure that amount is being reduced each frame, so we stabilise eventually
    ScreenShakeIntensity = ScreenShakeIntensity - ScreenShakeDuration
} else {
    //If we've stabilised or never had any screenshake
    ScreenShakeIntensity = 0
    ScreenShakeDuration = 0
    //Note the position the view 'should' be in
    x = view_xview
    y = view_yview
}

//View position should wobble away from normal position by screenshake amount
//If it's zero this should be perfectly stable
ShakeX = random_range(-ScreenShakeIntensity,ScreenShakeIntensity)
ShakeY = random_range(-ScreenShakeIntensity,ScreenShakeIntensity)

view_wview = global.ScreenWidth - abs(ShakeX)
view_hview = global.ScreenHeight - abs(ShakeY)
view_angle = 0 + (ShakeX/10)
view_xview = x + ShakeX
view_yview = y + ShakeY

